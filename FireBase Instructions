const res = await fetch(
          "https://reactform-b05f9-default-rtdb.firebaseio.com/reactform.json",
          {
            method: "POST",
            headers: {
              "Content-type": "application/json",
            },
            body: JSON.stringify({
              name,
              email,
              password,
              cpassword,
            }),
          }
        );

